{
    "manifest_version" : 2,
    "name" : "Selenium Java Robot debug helper",
    "description" : "This extension helps debugging with Chrome and the Selenium Java Robot",
    "version" : "1.0",
    "permissions" : ["<all_urls>", "tabs"],
    "background" : {
        "scripts" : ["background.js"]
    },
    "content_security_policy" : "script-src 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources" : ["page.js"]
}